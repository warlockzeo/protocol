<IfModule mod_headers.c>
    Header set Access-Control-Allow-Origin "*"
</IfModule>

<IfModule mod_rewrite.c>
	RewriteEngine on
	# Bloqueia a listagem de diret√≥rios.
	Options All -Indexes

	# Mensagem de erro personalizada.
	ErrorDocument 403 "Desculpe! Acesso nao permitido

	RewriteRule ^api/users/?$ /api/ClassUsers.php [NC]
	RewriteRule ^api/protocolos/?$ /api/ClassProtocol.php [NC]
	
	RewriteRule ^([a-z0-9-]+)/([a-z0-9-]+)/?$ /index.html [NC]
	RewriteRule ^([a-z0-9-]+)/?$ /index.html [NC]
	
</IfModule>


